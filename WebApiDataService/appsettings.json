{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:dd-MM-yyyy HH:mm:ss.fff} {Level:u3}] <{SourceContext}> {Message:lj} {NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "urls": "https://*:5003",
  "AllowedOrigins": "*"
}
